    <Routine name="feedback">
      <CodeComponent name="code">
        <Param name="Begin Experiment" updates="constant" val="" valType="extendedCode"/>
        <Param name="name" updates="None" val="code" valType="code"/>
        <Param name="Begin Routine" updates="constant" val="if trial_type == &quot;peak_interval&quot;:&amp;#10;    continueRoutine = False&amp;#10;else:&amp;#10;    pass" valType="extendedCode"/>
        <Param name="End Routine" updates="constant" val="# output&amp;#10;trial_responsetimes = trial_responsetimes[~np.isnan(trial_responsetimes)]&amp;#10;&amp;#10;for t in trial_responsetimes:&amp;#10;    # put in all the other info we want for a trial, too.&amp;#10;    thisExp.addData(&quot;trial_number&quot;, trial_number)&amp;#10;    thisExp.addData(&quot;trial_type&quot;, trial_type)&amp;#10;    thisExp.addData(&quot;response_time&quot;, t)&amp;#10;    thisExp.addData(&quot;score&quot;, score)&amp;#10;    thisExp.addData(&quot;reward&quot;, reward)&amp;#10;    thisExp.addData(&quot;cost&quot;, cost_per_response)&amp;#10;    thisExp.nextentry()" valType="extendedCode"/>

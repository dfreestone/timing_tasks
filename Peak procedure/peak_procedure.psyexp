        <Param name="End Routine" updates="constant" val="" valType="extendedCode"/>
        <Param name="End Experiment" updates="constant" val="" valType="extendedCode"/>
        <Param name="Each Frame" updates="constant" val="if len(fixed_response) &gt; nresponses:&amp;#10;    trial_responsetimes[nresponses] = fixed_response.rt[-1]&amp;#10;    if fixed_response.rt[-1] &gt;= fi_duration:&amp;#10;        score += reward&amp;#10;        continueRoutine = False&amp;#10;    else:&amp;#10;        score -= cost_per_response&amp;#10;    nresponses += 1" valType="extendedCode"/>
